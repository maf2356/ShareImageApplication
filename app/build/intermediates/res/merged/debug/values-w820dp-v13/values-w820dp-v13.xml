<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="reference" name="SwipeBackLayoutStyle"/>
    <declare-styleable name="CircleImageView">
        <attr format="dimension" name="border_width"/>
        <attr format="color" name="border_color"/>
    </declare-styleable>
    <declare-styleable name="SwipeBackLayout">
        <attr format="dimension" name="edge_size"/>
        <attr name="edge_flag">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
            <enum name="bottom" value="2"/>
            <enum name="all" value="3"/>
        </attr>
        <attr format="reference" name="shadow_left"/>
        <attr format="reference" name="shadow_right"/>
        <attr format="reference" name="shadow_bottom"/>
    </declare-styleable>
    <dimen name="activity_horizontal_margin">64dp</dimen>
</resources>